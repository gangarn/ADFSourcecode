{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nareshdf12"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CopyPipeline_s8h')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_s8h",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "Test/UsedCars.csv"
							},
							{
								"name": "Destination",
								"value": "dbo.USEDCARS"
							}
						],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureDataLakeStoreReadSettings",
									"recursive": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings",
									"skipLineCount": 0
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Price",
											"type": "String"
										},
										"sink": {
											"name": "Price",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "Age",
											"type": "String"
										},
										"sink": {
											"name": "Age",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "KM",
											"type": "String"
										},
										"sink": {
											"name": "KM",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "FuelType",
											"type": "String"
										},
										"sink": {
											"name": "FuelType",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "HP",
											"type": "String"
										},
										"sink": {
											"name": "HP",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "MetColor",
											"type": "String"
										},
										"sink": {
											"name": "MetColor",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "Automatic",
											"type": "String"
										},
										"sink": {
											"name": "Automatic",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "CC",
											"type": "String"
										},
										"sink": {
											"name": "CC",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "Doors",
											"type": "String"
										},
										"sink": {
											"name": "Doors",
											"type": "Double"
										}
									},
									{
										"source": {
											"name": "Weight",
											"type": "String"
										},
										"sink": {
											"name": "Weight",
											"type": "Double"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_s8h",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_s8h",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_s8h')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SourceDataset_s8h')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStore1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureDataLakeStoreLocation",
						"fileName": "UsedCars.csv",
						"folderPath": "Test"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Price",
						"type": "String"
					},
					{
						"name": "Age",
						"type": "String"
					},
					{
						"name": "KM",
						"type": "String"
					},
					{
						"name": "FuelType",
						"type": "String"
					},
					{
						"name": "HP",
						"type": "String"
					},
					{
						"name": "MetColor",
						"type": "String"
					},
					{
						"name": "Automatic",
						"type": "String"
					},
					{
						"name": "CC",
						"type": "String"
					},
					{
						"name": "Doors",
						"type": "String"
					},
					{
						"name": "Weight",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		}
	]
}